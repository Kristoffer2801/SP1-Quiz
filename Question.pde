class Question
{
  String question;
  String optionA;
  String optionB;
  String optionC;
  String optionD;
  String correctAnswer;
  
  Question(String question, String optionA, String optionB, String optionC, String optionD, String correctAnswer)
  {
    this.question = question;
    this.optionA = optionA;
    this.optionB = optionB;
    this.optionC = optionC;
    this.optionD = optionD;
    this.correctAnswer = correctAnswer;
  }
}
